# 环境变量配置示例
# 复制此文件为 .env 并填入实际值

# OpenAI API配置 (可选，用于AI增强功能)
OPENAI_API_KEY=your_openai_api_key_here

# MLT Framework路径
MELT_PATH=/Applications/kdenlive.app/Contents/MacOS/melt

# 项目配置
PROJECT_NAME=Kdenlive Effect Generator
DEBUG=true

# 预览视频配置
PREVIEW_WIDTH=720
PREVIEW_HEIGHT=1280
PREVIEW_DURATION=5
PREVIEW_FPS=25

# Web服务器配置
WEB_HOST=localhost
WEB_PORT=5000
